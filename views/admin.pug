extends layout_admin

block content
  div(class='loading')
    span Loading...
  
  div(class='board-container')
    div(class='board-zoomer')
      div(class='board-mover')
        canvas(id='board', width=100, height=100)
  
  div(class='reticule', id='reticule')
  div(class='selection')

  div(class='palette')
  div(class='coords') (0, 0)
  div(class='bubble-container')

  div(class='ui', id='ui')
    include includes/admin_sidebar